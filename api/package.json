{
  "name": "famouspeople-api",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "cf:dev": "wrangler dev src/worker.ts",
    "cf:deploy": "wrangler deploy",
    "cf:deploy:production": "wrangler deploy --env production",
    "cf:secret:list": "wrangler secret list",
    "cf:login": "wrangler login"
  },
  "dependencies": {
    "@hono/node-server": "^1.13.7",
    "@supabase/supabase-js": "^2.45.1",
    "@upstash/ratelimit": "^1.1.4",
    "@upstash/redis": "^1.25.1",
    "dotenv": "^16.4.5",
    "hono": "^4.11.7",
    "pino": "^9.3.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "tsx": "^4.16.2",
    "typescript": "^5.7.2",
    "vitest": "^2.0.5",
    "wrangler": "^4.17.0"
  }
}
